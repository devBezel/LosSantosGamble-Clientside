{"code":"import * as alt from 'alt';\r\nimport { View } from '../Utilities/View';\r\nimport { Key } from 'client/modules/Constant/Keys/Key';\r\nexport default async () => {\r\n    let webView;\r\n    alt.on('keyup', async (key) => {\r\n        if (key === Key.ESCAPE) {\r\n            if (webView === null || webView === undefined) {\r\n                return;\r\n            }\r\n            webView.close();\r\n        }\r\n    });\r\n    alt.onServer('vehicle:openWindow', getDataVehicles);\r\n    async function getDataVehicles(vehicleList) {\r\n        openVehicleWindow();\r\n        webView.emit('cef:vehicleList', vehicleList);\r\n    }\r\n    async function openVehicleWindow() {\r\n        if (!webView) {\r\n            webView = new View();\r\n        }\r\n        if (alt.Player.local.getMeta('viewOpen'))\r\n            return;\r\n        webView.open('', true, 'character/vehicle');\r\n        webView.on('cef:vehicleSpawn', sendVehicleToServer);\r\n    }\r\n    async function sendVehicleToServer(vehicleId) {\r\n        webView.close();\r\n        alt.emitServer('vehicle:spawnVehicle', vehicleId);\r\n    }\r\n};\r\n","references":["B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Models/vehicle.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Core/Utilities/View.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/Keys/Key.ts"]}
