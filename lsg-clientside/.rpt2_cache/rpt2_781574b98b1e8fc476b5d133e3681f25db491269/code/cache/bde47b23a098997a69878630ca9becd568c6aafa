{"code":"import * as alt from 'alt';\r\nimport * as game from 'natives';\r\nimport { ServerEvent } from 'client/modules/Constant/ServerEvent';\r\nimport { CEFEvent } from 'client/modules/Constant/CEFEvent';\r\nimport { Browser } from '../Helpers/Browser';\r\nimport { Key } from 'client/modules/Constant/Keys/Key';\r\nexport default async () => {\r\n    const view = new alt.WebView('http://resource/assets/views/lsg_bus-stations/index.html');\r\n    alt.onServer(ServerEvent.BUS_STATION_LIST, async (station) => {\r\n        view.emit(CEFEvent.CEF_BUS_STATION_LIST, station);\r\n        Browser.browserSettings(view, true, true, false);\r\n    });\r\n    alt.onServer(ServerEvent.BUS_STATION_SELECTED, async (id, x, y, z, time) => {\r\n        view.emit(CEFEvent.CEF_BUS_TIMER, time);\r\n        view.on(CEFEvent.CEF_BUS_TIMER_END, async () => {\r\n            const localPlayerId = alt.Player.local.scriptID;\r\n            game.setEntityCoords(localPlayerId, x, y, z, false, false, false, false);\r\n            Browser.browserSettings(view, false, false, true);\r\n        });\r\n    });\r\n    alt.on('keyup', async (key) => {\r\n        if (key === Key.ESCAPE) {\r\n            view.emit(ServerEvent.BUS_STATION_LIST, false);\r\n            Browser.browserSettings(view, false, false, true);\r\n        }\r\n    });\r\n    view.on(CEFEvent.CEF_BUS_SELECT_STATION, async (id, x, y, z, cost, time) => {\r\n        alt.emitServer(ServerEvent.BUS_STATION_SELECTED, id, x, y, z, cost, time);\r\n    });\r\n};\r\n","references":["B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/ServerEvent.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/CEFEvent.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Core/Helpers/Browser.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/Keys/Key.ts"]}
