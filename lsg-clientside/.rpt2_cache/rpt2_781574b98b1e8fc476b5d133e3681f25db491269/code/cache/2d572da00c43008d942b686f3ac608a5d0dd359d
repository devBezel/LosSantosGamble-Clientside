{"code":"import * as alt from 'alt';\r\nimport { ServerEvent } from 'client/modules/Constant/ServerEvent';\r\nimport { CEFEvent } from 'client/modules/Constant/CEFEvent';\r\nimport { Browser } from '../../Helpers/Browser';\r\nimport { ClientEvent } from 'client/modules/Constant/ClientEvent';\r\nimport { Key } from 'client/modules/Constant/Keys/Key';\r\nexport default async () => {\r\n    const view = new alt.WebView('http://resource/assets/views/lsg_descriptions/index.html');\r\n    alt.onServer(ServerEvent.DESCRIPTION_LIST, async (descriptions) => {\r\n        view.emit(CEFEvent.CEF_DESCRIPTION_LIST, true, descriptions);\r\n        Browser.browserSettings(view, true, true, false);\r\n    });\r\n    view.on(CEFEvent.CEF_SET_DESCRIPTION, async (description) => {\r\n        alt.emitServer(ClientEvent.SET_DESCRIPTION, description);\r\n    });\r\n    view.on(CEFEvent.CEF_REMOVE_DESCRIPTION, async (id) => {\r\n        alt.emitServer(ClientEvent.REMOVE_DESCRIPTION, id);\r\n    });\r\n    view.on(CEFEvent.CEF_ADD_DESCRIPTION, async (title, content) => {\r\n        alt.emitServer(ClientEvent.ADD_DESCRIPTION, title, content);\r\n    });\r\n    view.on(CEFEvent.CEF_EDIT_DESCRIPTION, async (id, title, content) => {\r\n        alt.emitServer(ClientEvent.EDIT_DESCRIPTION, id, title, content);\r\n    });\r\n    alt.on('keyup', async (key) => {\r\n        if (key === Key.ESCAPE) {\r\n            view.emit(CEFEvent.CEF_DESCRIPTION_LIST, false);\r\n            Browser.browserSettings(view, false, false, true);\r\n        }\r\n    });\r\n};\r\n","references":["B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/ServerEvent.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/CEFEvent.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Core/Helpers/Browser.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/ClientEvent.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/Keys/Key.ts"]}
