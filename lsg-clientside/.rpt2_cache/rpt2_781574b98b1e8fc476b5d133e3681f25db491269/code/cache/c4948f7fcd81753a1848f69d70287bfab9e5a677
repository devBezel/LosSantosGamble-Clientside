{"code":"import * as alt from 'alt';\r\nimport { ServerEvent } from 'client/modules/Constant/ServerEvent';\r\nimport { Browser } from 'client/modules/Core/Helpers/Browser';\r\nimport { CEFEvent } from 'client/modules/Constant/CEFEvent';\r\nimport { Key } from 'client/modules/Constant/Keys/Key';\r\nexport default async () => {\r\n    const view = new alt.WebView('http://resource/assets/views/lsg_group-panel/index.html');\r\n    alt.onServer(ServerEvent.GROUP_OPEN_MENU, async (id, name, registered, salaryAccount, tag, membersCount, ownerName, groupMembers) => {\r\n        Browser.browserSettings(view, true, true, false);\r\n        alt.log(groupMembers[0].memberName);\r\n        view.emit(CEFEvent.CEF_GROUP_SHOW_VIEW, true, id, name, registered, salaryAccount, tag, membersCount, ownerName, groupMembers);\r\n    });\r\n    alt.on('keyup', async (key) => {\r\n        if (key === Key.ESCAPE) {\r\n            view.emit(CEFEvent.CEF_GROUP_SHOW_VIEW, false);\r\n            Browser.browserSettings(view, false, false, true);\r\n        }\r\n    });\r\n};\r\n","references":["B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/ServerEvent.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Core/Helpers/Browser.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/CEFEvent.ts","B:/Projekty/LSGamble-Clientside/lsg-clientside/modules/Constant/Keys/Key.ts"]}
